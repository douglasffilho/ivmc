{
  "name": "ivmc",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:douglasffilho/ivmc.git",
  "keywords": [
    "Instituto",
    "Vozes",
    "Minha",
    "Cabe√ßa"
  ],
  "author": "Douglas Fernandes <douglasf.filho@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest --maxWorkers=50% --forceExit --collectCoverage --detectOpenHandles",
    "test:dev": "jest --maxWorkers=50% --updateSnapshot --collectCoverage --detectOpenHandles",
    "coverage": "yarn test && google-chrome-stable coverage/lcov-report/index.html",
    "clean": "chmod +x clean && ./clean",
    "update-deps": "node updateAllDependencies.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "dependencies": {
    "@svgr/webpack": "6.2.1",
    "next": "12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/react-hooks": "8.0.0",
    "@testing-library/user-event": "14.1.1",
    "babel-jest": "28.0.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "26.1.5",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "git-cz": "4.8.0",
    "husky": "7.0.4",
    "jest": "28.0.0",
    "jest-axe": "6.0.0",
    "jest-environment-jsdom": "28.0.0",
    "jest-watch-typeahead": "1.1.0",
    "lint-staged": "13.1.1",
    "prettier": "2.6.2"
  }
}
